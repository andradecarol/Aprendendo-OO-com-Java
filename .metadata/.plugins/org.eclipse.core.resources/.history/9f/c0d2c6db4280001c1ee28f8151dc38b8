public class Conta {
	double saldo; //atributo
	int agencia; //atributo
	int numero; //atributo
	String titular; //atributo
	
	public void deposita(double valor) { //mÃ©todo
		this.saldo = this.saldo + valor;
	}
	
	public boolean saca(double valor) {
		if(this,saldo >= valor) {
			this.saldo = this.saldo - valor;
			return true;
		} else {
			return false;
		}
	}
}